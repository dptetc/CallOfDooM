// Individual Bits and Pieces
actor GIB1
{
	+DOOMBOUNCE
	PROJECTILE
	-NOGRAVITY
	-NOTELEPORT
	-SOLID
	-TELESTOMP
	Radius 2
	Damage 0
	Decal BloodSplat
	bouncefactor 0.4
	SeeSound "gibbage/xgib"
	Speed 5
	renderstyle translucent
	alpha 1.0
	States
	{
	Spawn:
		GIB1 A 0
		GIB1 A 0 A_Jump(64,"Alt") 
		GIB1 A 0 ThrustThingZ (0, random(0,45), 0, 1)
		goto See
	Alt:
		GIB1 A 0 A_SpawnItemEx ("GIB1SPLAT", 1, 1, random(8,32), random(0,5), random(0,5), random(8,18), random(0,360), 128, 0)
		Stop 
	See:
		NULL A 0 A_JumpIf(waterlevel > 1, "Remove")
		GIB1 A 5 A_Custommissile("Blood", -5,0,0)
		GIB1 B 5 //A_Fadeout(0.1)
		GIB1 AB 5
		loop
	Death:
		GIB1 A 0 A_ChangeFlag(NOBLOCKMAP, 0)
		GIB1 A 0 A_Jump (64,2)
		GIB1 A 0 A_Jump (128,2)
		GIB1 A 200
		Stop
		GIB1 B 200
		Stop
	Remove: // Anti-Lag - Removes blood in water.
		NULL A 0 
		stop
	}
}

