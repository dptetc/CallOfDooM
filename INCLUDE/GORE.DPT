actor NashGore_Blood : Blood replaces Blood
{
	+PUFFGETSOWNER // we need this this flag for everything to work
	-TELESTOMP
	states
	{
	Spawn:
		BLUD C 0
		BLUD C 8 A_SpawnItemEx("NashGore_FlyingBlood", 1, 1, random(8,32), random(0,5), random(0,5), random(8,18), random(0,360), 161, 0)
		BLUD B 8 
		BLUD A 300 
		stop
	}
}

actor NashGore_FlyingBlood
{
	game Doom
	scale 0.75
	health 1
	radius 8
	height 1
	mass 1
	renderstyle translucent
	alpha 1.0
	+CORPSE
	+NOTELEPORT
	+NOBLOCKMAP
	-TELESTOMP
	states
	{
	Spawn:
		TNT1 A 0 A_CheckSight("Cheap")
		FBLD A 1 A_SpawnItem("NashGore_FlyingBloodTrail",0,0,0,1)
		FBLD A 1 A_JumpIf(waterlevel > 1, "Remove")
		//  FBLD A 0 A_Fadeout(0.1)
		loop
	Cheap:
		FBLD A 8 A_SpawnItem("NashGore_FlyingBloodTrail",0,0,0,1)
		FBLD A 1 A_JumpIf(waterlevel > 1, "Remove")
		//  FBLD A 0 A_Fadeout(0.1)
		loop
	Crash:
		TNT1 A 1 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
		stop
	Remove: // Anti-Lag - Removes blood in water.
		NULL A 0 
		stop
	}
}

actor NashGore_BloodSpot
{
	game Doom
	radius 12
	height 2
	mass 1
	renderstyle translucent
	alpha 1.0
	+NOTELEPORT
	-NOBLOCKMAP
	-TELESTOMP
	states
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_PlaySound("gibbage/blood")
		TNT1 A 0 A_Jump(64,4)
		TNT1 A 0 A_Jump(128,4)
		TNT1 A 0 A_Jump(192,4)
		TNT1 A 0 A_Jump(255,4)
		// make the blood spots stay on the ground. 1500 is the default duration.
		BSPT A 200 A_JumpIf(waterlevel > 1, "Remove")
		stop
		BSPT B 200 A_JumpIf(waterlevel > 1, "Remove")
		stop
		BSPT C 200 A_JumpIf(waterlevel > 1, "Remove")
		stop
		BSPT D 200 A_JumpIf(waterlevel > 1, "Remove")
		stop
	Remove: // Anti-Lag - Removes blood in water.
		NULL A 0 
		stop
	}
}

// Blood trails spawned by FlyingBlood
actor NashGore_FlyingBloodTrail
{
	game Doom
	scale 0.70
	mass 1
	+LOWGRAVITY
	+NOTELEPORT
	+NOBLOCKMAP
	-TELESTOMP
	states
	{
	Spawn:
		BTRL A 4 A_JumpIf(waterlevel > 1, "Remove")
		BTRL B 4 A_JumpIf(waterlevel > 1, "Remove")
		BTRL C 4 A_JumpIf(waterlevel > 1, "Remove")
		BTRL D 4 A_JumpIf(waterlevel > 1, "Remove")
		stop
	Remove: // Anti-Lag - Removes blood in water.
		NULL A 0 
		stop
	}
}

