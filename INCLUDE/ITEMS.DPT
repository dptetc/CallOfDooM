		TNT1 A 0 A_SpawnItemEx("Ember1", 0, 0, 0, 0, 0, 0, 0, 128)
		Loop
	}
}

ACTOR ArchVileFireSpawner : ArchVileFire
{
	Height 16
	Radius 21
	Speed 21
	Damage 7
	PROJECTILE
	+ClientSideOnly
	+NoBlockMap
	+NoGravity
	+MISSILE
	seesound "fire/loop"
	States
	{
	Spawn:
		TNT1 A 0 A_SpawnItemEx("Ember1", 0, 0, 0, 0, 0, 0, 0, 128)
