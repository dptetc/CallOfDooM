ACTOR ExplosionSmoke
{
	Radius 0
	Height 0
	Alpha 0.2
	RenderStyle Translucent
	Scale 0.8
	Speed 2
	+NOBLOCKMAP
	+NOTELEPORT
	+DONTSPLASH
	+MISSILE
	+FORCEXYBILLBOARD
	+CLIENTSIDEONLY
	Damage 0
	+NOINTERACTION
	States
	{
	Spawn:
		SMOK A 0
		Goto Death
	Death:
		SMOC ABCDEFGHIJKLMNOPQRSTUVWXYZ 4 A_FadeOut(0.01)
		SM7C AB 4 A_FadeOut(0.01)
		SMOC ABCDEFGHIJKLMNOPQRSTUVWXYZ 4 A_FadeOut(0.01)
		SM7C AB 4 A_FadeOut(0.01)
		Stop
	}
}

ACTOR AltSmoke: ExplosionSmoke
{
	+NOINTERACTION
	+NOCLIP
	Scale 0.8
	States
	{
	Spawn:
		PUF2 A 1
		Goto Death
	Death:                                                          
		PUF2 ABCDEFGHIJKLMNOPQRSTUVWXYZ 2 A_FadeOut(0.010)
		Stop
	}
}

Actor PlasmaSmoke: AltSmoke
{
	Alpha 0.15
	Speed 1
	+CLIENTSIDEONLY
	Scale 0.4
	States
	{
	Spawn:
		PUF2 D 1
		Goto Death
	Death:                                                          
		SMOC ABCDEFGHIJKLMNOPQRSTUVWXYZ 2 A_FadeOut(0.002)
		SM7C AB 2 A_FadeOut(0.002)
		SMOC ABCDEFGHIJKLMNOPQRSTUVWXYZ 2 A_FadeOut(0.002)
		SM7C AB 2 A_FadeOut(0.002)
		Stop
	}
}

Actor DebrisGeneral
{
	+MISSILE
	+NOBLOCKMAP
	+NOGRAVITY
	+DROPOFF
	+NOTELEPORT
	+FORCEXYBILLBOARD
	+NOTDMATCH
	+GHOST
	radius 1
	height 1
	mass 1
	damage 0
}

Actor SmokeSpawner : DebrisGeneral
{
	Speed 20
	+NOCLIP
	States
	{
	Spawn:
		TNT1 A 1
		TNT1 A 0 Thing_ChangeTID(0,390)
		TNT1 A 0 A_SpawnItem("ShotSmoke",0,0)
		Stop
	}
}

actor ShotSmoke
{
	+NOTDMATCH
	+NOINTERACTION
	vspeed 1
	renderstyle Add
	alpha 0.3
	scale 0.1
	states
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 Thing_ChangeTID(0,390)
		SMOK ABCDEFGHIJKLMNOPQ 1 A_SpawnItem("ShotSmokeTrail") //A_FadeOut(0.01)
		stop
	}
}

Actor ShotSmokeTrail : ShotSmoke
{
	vspeed 0
	alpha 0.15
	states
	{
	Spawn:
		SMOK CDEFGHIJKLMNOPQ 1
		stop
	}
}

Actor Rocket1Trail
{
	+NOTDMATCH
	+NOCLIP +NOBLOCKMAP
	+NOGRAVITY
	+NOINTERACTION
	scale 0.1
	renderstyle translucent
	alpha 0.4
	states
	{
	Spawn:
		SMOK ABCDEFGHIJKLMNOPQ 1 Thing_ChangeTID(0,390)
		stop
	}
}

Actor SmokingPiece : DebrisGeneral
{
	-NOGRAVITY
	speed 12
	gravity 0.7
	states
	{
	Spawn:
		TNT1 A 0
		TNT1 A 1 Thing_ChangeTID(0,390)
		TNT1 A 1 A_SpawnItem("SmokingPieceSmoke")
		wait
	}
}

Actor SmokingPieceSmoke : ShotSmokeTrail
{
	scale 0.06
	alpha 0.5
}

Actor ExplosiveBarrel1 : ExplosiveBarrel replaces ExplosiveBarrel
{
	states
	{
	Spawn:
		BAR1 AB 4
		loop
	Death:
		BEXP AB 2
		BEXP C 2 A_Scream
		BEXP D 2 A_Explode
		TNT1 A 0 Radius_Quake(3,15,0,5,0)
		//TNT1 AAAAAAA 0 A_custommissile("SmokingPiece",random(0,16),random(-8,8),random(-360,360),2,random(10,70))
		//TNT1 AAAAAA 0 A_custommissile("BarrelPart",20,random(-5,5),random(0,360),2,random(40,80))
		//TNT1 A 0 A_SpawnItem("XBarrelChunk",0,0)
		BEXP EFGHIJKLMNOPQ 2
		BEXP Q 1050 A_BarrelDestroy
		BEXP Q 5 A_Respawn
		wait
	}
}

ACTOR XBarrelChunk
{
	States
	{
	Spawn:
		BEXP RSTRSTRSTRSTRSTRST 7 BRIGHT
		BEXP RSTRST 8 BRIGHT
		BEXP RSTRST 9 BRIGHT
		BEXP RSTRSTRSTRRSSTTRRSSTTRRRSSSTTTRRRRSSSSTTTT 10
		BEXP U -1
		stop
	}
}

Actor BarrelPart : SmokingPiece
{
	-ROCKETTRAIL
	+DOOMBOUNCE
	renderstyle translucent
	alpha 1.0
	scale 0.85
	-NOBLOCKMAP
	gravity 1.0
	seesound "bounce"
	mass 1
	states
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 Thing_ChangeTID(0,390)
		BPRT ABCDEFGH 2 A_SpawnItem("ShotSmoke")
		loop
	Death:
		TNT1 A 0
		TNT1 A 0 A_jump(255,"death1","death2","death3","death4","death5","death6")
	Death1:
		BPRT A 1 A_Fadeout(0.001)
		loop
	Death2:
		BPRT C 1 A_Fadeout(0.001)
		loop
	Death3:
		BPRT E 1 A_Fadeout(0.001)
		loop
	Death4:
		BPRT G 1 A_Fadeout(0.001)
		loop
	Death5:
		BPRT B 1 A_Fadeout(0.001)
		loop
	Death6:
		BPRT D 1 A_Fadeout(0.001)
		loop
	}
}

actor FireSpawner : DoomImpBall 10000
{
	DamageType "Fire"
	Height 16
	Radius 21
	Speed 21
	Damage 7
	PROJECTILE
	+NoBlockMap
	+NoGravity
	-MISSILE
	seesound "fire/loop"
